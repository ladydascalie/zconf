		"planetscale-context-server": {
			"enabled": true,
			"settings": {
				"context_servers": {
					"planetscale-context-server": {
						"settings": {
							"organization": "lootlocker",
							"database": "game_backend",
						},
					},
				},
			},
		},
	},
	"features": {
		"edit_prediction_provider": "zed",
	},
	"languages": {
		"Go": {
			"formatter": "language_server",
			"tab_size": 8,
		},
		"YAML": {
			"tab_size": 2,
		},
		"JSONC": {
			"tab_size": 4,
		},
