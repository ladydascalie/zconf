{
	"buffer_line_height": "comfortable",
	"outline_panel": {
		"auto_fold_dirs": true,
	},
	"agent_servers": {
		"github-copilot": {
			"type": "registry",
		},
	},
	"redact_private_values": true,
	"git": {
		"inline_blame": {
			"show_commit_summary": false,
		},
	},
	"tabs": {
		"show_diagnostics": "errors",
		"file_icons": true,
		"git_status": true,
	},
	"cursor_shape": "bar",
	"edit_predictions": {
		"provider": "zed",
		"mode": "eager",
		"enabled_in_text_threads": false,
	},
	"project_panel": {
		"dock": "right",
		"entry_spacing": "standard",
	},
	"language_models": {},
	"agent": {
		"dock": "left",
		"play_sound_when_agent_done": true,
		"default_model": {
			"provider": "zed.dev",
			"model": "claude-sonnet-4-5",
		},
		"commit_message_model": {
			"provider": "copilot_chat",
			"model": "claude-sonnet-4",
		},
	},
	"icon_theme": "Catppuccin Macchiato",
	"ui_font_family": "MonoLisaCustom Nerd Font",
	"ui_font_weight": 400,
	"ui_font_size": 16,
	"bottom_dock_layout": "contained",
	"buffer_font_family": "MonoLisaCustom Nerd Font",
	"buffer_font_size": 16.0,
	"buffer_font_weight": 400.0,
	"line_indicator_format": "long",
	"terminal": {
		"cursor_shape": "bar",
		"toolbar": {
			"breadcrumbs": false,
		},
		"font_family": "MonoLisaCustom Nerd Font",
	},
	"hard_tabs": true,
	"tab_size": 8,
	"wrap_guides": [80, 120],
	"minimap": { "show": "always" },
	"theme": {
		"mode": "dark",
		"light": "One Light",
		"dark": "Nightfox - opaque",
	},
	"vim_mode": true,
	"lsp": {
		"golangci-lint": {
			"initialization_options": {
				"command": [
					"golangci-lint",
					"run",
					"--output.json.path=stdout",
					"--show-stats=false",
				],
			},
		},
		"gopls": {
			"initialization_options": {
				"build.buildFlags": ["-tags=mage,integration"],
				"formatting.gofumpt": true,
				"analyses": {
					"unusedvariable": true,
					"useany": true,
					"modernize": true,
					"ST1000": false,
				},
				"staticcheck": true,
				"ui.codelenses": {
					"run_vulncheck_exp": true,
				},
				"hints": {
					"assignVariableTypes": false,
					"compositeLiteralFields": true,
					"compositeLiteralTypes": true,
					"constantValues": true,
					"functionTypeParameters": true,
					"parameterNames": true,
					"rangeVariableTypes": true,
				},
			},
		},
	},
	"context_servers": {
		"serena-context-server": {
   "enabled": true,
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/oraios/serena",
				"serena",
				"start-mcp-server",
				"--context",
				"ide-assistant",
			],
		},
		"planetscale-context-server": {
   "enabled": true,
			"settings": {
				"context_servers": {
					"planetscale-context-server": {
						"settings": {
							"organization": "lootlocker",
							"database": "game_backend",
						},
					},
				},
			},
		},
	},
	"languages": {
		"Go": {
			"formatter": "language_server",
			"tab_size": 8,
		},
		"YAML": {
			"tab_size": 2,
		},
		"JSONC": {
			"tab_size": 4,
		},
	},
}
